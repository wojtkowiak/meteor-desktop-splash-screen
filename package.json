{
    "name": "meteor-desktop-splash-screen",
    "version": "0.0.6",
    "engines": {
        "node": ">=6.0.0"
    },
    "description": "Meteor Desktop splash screen plugin.",
    "main": "dist/index.js",
    "scripts": {
        "test": "npm run build && ava test/functional/**/*.js --source dist/**/* --verbose -s",
        "test-watch": "npm run build && ava test/functional/**/*.js --source dist/**/* --watch --verbose -s",
        "prepublish": "npm run build",
        "build": "babel src --out-dir dist --source-maps inline --copy-files",
        "build-watch": "babel src --watch --out-dir dist --source-maps inline --copy-files"
    },
    "author": "Bartosz Wojtkowiak <bartosz@wojtkowiak.it>",
    "license": "MIT",
    "devDependencies": {
        "ava": "^0.15.1",
        "babel-cli": "^6.9.0",
        "babel-core": "^6.9.0",
        "babel-eslint": "^6.0.4",
        "babel-preset-node6": "^11.0.0",
        "eslint": "^2.10.2",
        "eslint-config-airbnb-base": "^3.0.1",
        "eslint-plugin-import": "^1.8.0",
        "spectron": "^3.1.0",
        "shelljs": "^0.7.0",
        "electron-packager": "^7.0.2",
        "electron-prebuilt": "^1.1.2",
        "node-resemble-js": "^0.0.5",
        "meteor-desktop-plugin-test-suite": "0.0.3"
    },
    "dependencies": {
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wojtkowiak/meteor-electron-desktop-client"
    },
    "keywords": [
        "electron",
        "meteor",
        "packager",
        "desktop",
        "splash",
        "splashscreen"
    ],
    "bugs": {
        "url": "https://github.com/wojtkowiak/meteor-electron-desktop-client/issues"
    },
    "homepage": "https://github.com/wojtkowiak/meteor-electron-desktop-client"
}
